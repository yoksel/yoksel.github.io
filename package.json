{
  "name": "yoksel.github.io",
  "private": true,
  "description": "Blog",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby clean && node src/add-stars.js && gatsby build",
    "develop": "gatsby develop",
    "deploy": "npm run build && gh-pages -d public -b master --dotfiles true",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "npm run js:lint && npm run css:lint",
    "lint:fix": "npm run js:fix && npm run css:fix",
    "js:lint": "eslint src",
    "js:fix": "eslint src --fix",
    "css:lint": "stylelint **/*.scss",
    "css:fix": "stylelint **/*.scss --fix"
  },
  "dependencies": {
    "gatsby": "^2.23.3",
    "gatsby-plugin-sass": "^2.3.5",
    "gatsby-remark-embedder": "^2.1.1",
    "gatsby-remark-prismjs": "^3.5.5",
    "gatsby-source-filesystem": "^2.3.13",
    "gatsby-transformer-remark": "^2.8.18",
    "node-sass": "^4.14.1",
    "prismjs": "^1.20.0",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "disqus-react": "1.0.9",
    "dotenv": "8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-standard": "^4.0.1",
    "gatsby-plugin-feed": "^2.5.10",
    "gatsby-plugin-no-javascript": "^2.0.5",
    "gatsby-plugin-no-sourcemaps": "^2.2.0",
    "gatsby-plugin-preact": "4.0.9",
    "gatsby-plugin-react-helmet": "^3.3.9",
    "gatsby-transformer-yaml": "^2.4.6",
    "gh-pages": "^3.1.0",
    "nanoid": "3.1.10",
    "node-fetch": "^2.6.0",
    "p-limit": "^3.0.2",
    "preact": "10.4.6",
    "prop-types": "^15.7.2",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "stylelint": "^13.0.0",
    "stylelint-config-standard": "^19.0.0",
    "tiny-glob": "^0.2.6",
    "unsplash-js": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yoksel/yoksel.github.io"
  },
  "bugs": {
    "url": "https://github.com/yoksel/yoksel.github.io/issues"
  },
  "homepage": "https://yoksel.github.io"
}
