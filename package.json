{
  "name": "yoksel.github.io",
  "private": true,
  "description": "Blog",
  "version": "0.2.0",
  "license": "MIT",
  "packageManager": "yarn@4.1.0",
  "scripts": {
    "build-old": "gatsby clean && node src/add-stars.js && gatsby build",
    "develop": "gatsby develop",
    "deploy": "npm run build && gh-pages -d public -b master --dotfiles true",
    "start-old": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "lint-old": "npm run js:lint && npm run css:lint",
    "lint:fix": "npm run js:fix && npm run css:fix",
    "js:lint": "eslint src",
    "js:fix": "eslint src --fix",
    "css:lint": "stylelint **/*.scss",
    "css:fix": "stylelint **/*.scss --fix",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier 'src/**/*.ts*' --write",
    "getPosts": "ts-node src/utils/collectPostsUrls.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "next": "14.1.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yoksel/yoksel.github.io"
  },
  "bugs": {
    "url": "https://github.com/yoksel/yoksel.github.io/issues"
  },
  "homepage": "https://yoksel.github.io",
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.25",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/addon-onboarding": "^8.0.4",
    "@storybook/blocks": "^8.0.4",
    "@storybook/nextjs": "^8.0.4",
    "@storybook/react": "^8.0.4",
    "@storybook/test": "^8.0.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "classnames": "^2.5.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-storybook": "^0.8.0",
    "gh-pages": "^3.1.0",
    "gray-matter": "^4.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "storybook": "^8.0.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
